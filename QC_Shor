        "qc.measure(range(num_ancilla), range(num_ancilla))\n",
        "\n",
        "# Simulate the circuit using the Aer simulator\n",
        "simulator = AerSimulator()\n",
        "job = simulator.run(transpile(qc, simulator), shots=1024)\n",
        "result = job.result()\n",
